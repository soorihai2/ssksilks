{
  "name": "new-2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist public && vite build && mkdir -p public && mv dist/* public/ && rm -rf dist",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
   
    "build:prod": "rimraf dist public && vite build --mode production && mkdir -p public && mv dist/* public/ && rm -rf dist",
    "serve:prod": "vite preview --port 4173",
    "analyze": "vite build --mode analyze",
    "clean": "rimraf dist public",
    "type-check": "tsc --noEmit",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "server": "cd backend && npm run dev",
    "start": "concurrently \"cd backend && npm run dev\" \"npm run dev\"",
    "prod:build": "npm run build:prod",
    "prod:preview": "vite build --mode preview && concurrently \"vite preview --port 4173 --mode preview\" \"cd backend && npm run preview\" --kill-others",
    "prod:all": "npm run prod:build && npm run prod:preview",
    "preview": "vite preview --mode preview",
    "build:preview": "vite build --mode preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.17.1",
    "@mui/material": "^5.17.1",
    "@reduxjs/toolkit": "^2.2.1",
    "@tanstack/react-query": "^5.17.19",
    "@types/nodemailer": "^6.4.17",
    "@types/papaparse": "^5.3.15",
    "@types/uuid": "^10.0.0",
    "axios": "^1.8.4",
    "chart.js": "^4.4.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "framer-motion": "^11.0.3",
    "http-proxy-middleware": "^3.0.5",
    "mongoose": "^8.1.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.9",
    "papaparse": "^5.4.1",
    "razorpay": "^2.9.2",
    "react": "^18.2.0",
    "react-barcode": "^1.3.2",
    "react-chartjs-2": "^5.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.0",
    "react-toastify": "^10.0.4",
    "react-use": "^17.4.2",
    "react-use-gesture": "^9.1.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "rimraf": "^5.0.10",
    "terser": "^5.39.0",
    "typescript": "^5.3.3",
    "vite": "^5.4.18"
  }
}
